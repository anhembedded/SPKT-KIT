.................... int1 inputcd(int16 pin) 
.................... { 
....................       int8 n; 
....................       static int16 oldpin=0xffff; 
....................       static int1 tt=0; 
....................       static int1 dem=0; 
....................       if((input(pin)==0)&&(dem==0)) 
....................        { 
....................          if(tt==0) {oldpin = pin;tt=1;} 
....................          if(pin==oldpin)  
....................           { 
....................            for(n=0;n<5;n++)hien_thi_8led_7doan_quet_all(); 
....................            dem=1; 
....................            return 0; 
....................           } 
....................       } 
....................       else if(input(pin)&&dem) 
....................       { 
....................           if(pin==oldpin) 
....................           {    
....................               for(n=0;n<5;n++)hien_thi_8led_7doan_quet_all(); 
....................               if(input(pin)) 
....................               { 
....................                  tt=0;  
....................                  dem=0; 
....................                  oldpin=0xffff; 
....................               } 
....................           } 
....................       } 
....................       return 1;  
.................... } 

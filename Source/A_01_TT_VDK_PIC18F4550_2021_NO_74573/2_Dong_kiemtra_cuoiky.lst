....................          xh=rom_code[i]/16;               
....................          if(xh>9) xh=xh+0x37; 
....................          else     xh=xh+0x30;           
....................          xl=rom_code[i]%16;               
....................          if(xl>9) xl=xl+0x37; 
....................          else     xl=xl+0x30; 
